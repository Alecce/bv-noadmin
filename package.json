    "@types/ol": "^6.5.3",
    "@types/proj4": "^2.5.2",
    "angular2-yandex-maps": "^1.0.42",
    "bootstrap": "^4.6.0",
    "express": "^4.15.2",
    "file-saver": "^2.0.5",
    "glob-parent": "^6.0.0",
