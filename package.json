{
  "name": "bv",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --max-old-space-size=16192",
    "build": "ng build --max-old-space-size=8192",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer bv/dist/bv/stats.json",
    "myCustomMemoryComm": "node --max_old_space_size=4192 ./node_modules/@angular/cli/bin/ng",
    "build:client-and-server-bundles": "ng build && ng run bv:server",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:ssr2": "npm run compile:server",
    "build:ssr3": "ng run bv:server",
    "compile:server": "tsc -p tsconfig.server.json",
    "generate:prerender": "cd dist && node prerender",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node bv/dist/bv-noadmin/server/main.js"
  },
  "private": true,
